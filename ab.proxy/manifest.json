 
{
    "manifest_version": 2,
    "name": "Proxy Extension",
    "version": "1.0",
    "permissions": [
      "proxy",
      "storage"
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    }
  }
  